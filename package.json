{
  "name": "cd-form-library",
  "version": "0.1.97",
  "description": "Webflow-native form enhancement library using Custom Attributes",
  "main": "dist/browser.js",
  "unpkg": "dist/browser.js",
  "scripts": {
    "build": "esbuild src/browser.ts --bundle --outfile=dist/browser.js --format=iife --global-name=CDFormLibrary --minify",
    "build:dev": "esbuild src/browser.ts --bundle --outfile=dist/browser.js --format=iife --global-name=CDFormLibrary --sourcemap",
    "build:clean": "rm -rf dist && npm run build",
    "dev": "esbuild src/browser.ts --bundle --outfile=dist/browser.js --format=iife --global-name=CDFormLibrary --watch"
  },
  "keywords": [
    "webflow",
    "forms",
    "custom-attributes"
  ],
  "author": "CorporateDirect",
  "license": "MIT",
  "dependencies": {
    "@maskito/core": "^3.10.3",
    "@maskito/kit": "^3.10.3"
  },
  "devDependencies": {
    "esbuild": "^0.25.9",
    "typescript": "^5.0.0"
  },
  "files": [
    "dist"
  ],
  "types": "dist/browser.d.ts"
}
